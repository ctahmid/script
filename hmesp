Hide.MouseButton1Click:Connect(function()
	if Hide.Text == ">" then
		Hide.Text = "<"
		Frame:TweenPosition(UDim2.new(0.8, 370, 0.3, 31), "Out", "Quint", 1, true)
	else
		Hide.Text = ">"
		Frame:TweenPosition(UDim2.new(0.8, -2, 0.3, 31), "Out", "Quint", 1, true)
	end
end)

